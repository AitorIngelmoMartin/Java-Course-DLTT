package ClassesDelTutorial;

public class MultiplicationTable{
	void print(int table) {
		for(int i =1;1<=10;i++){		
			System.out.printf("%d * %d = %d", table, i, table*i).println();
		}	
	}
	void print(int table, int from, int to) {
		for(int i =from;i<=to;i++){		
			System.out.printf("%d * %d = %d", table, i, table*i).println();
		}	
	}
}